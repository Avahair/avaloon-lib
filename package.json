{
  "name": "@avaloon/lib",
  "version": "0.0.6",
  "description": "The platform is an online beauty and wellness marketplace.",
  "author": "Funmi Ayinde",
  "private": false,
  "license": "UNLICENSED",
  "files": [
    "dist/**/**",
    "*.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Avahair/avaloon-lib"
  },
  "bugs": "https://github.com/Avahair/avaloon-lib",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "publish-public": "npm publish --access public",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": " ts-node -r tsconfig-paths/register --transpile-only src/migrate.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/ormConfig.ts",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.7.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.4",
    "@nestjs/mongoose": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.7.8",
    "@types/ioredis": "^4.27.2",
    "amqplib": "^0.6.0",
    "auth0-id-generator": "^0.1.0",
    "aws-sdk": "^2.817.0",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "debug": "^4.3.2",
    "google-libphonenumber": "^3.2.26",
    "ioredis": "^4.27.9",
    "json-pick-keys": "0.0.4",
    "lodash": "^4.17.21",
    "memcached": "^2.2.2",
    "moment-timezone": "^0.5.32",
    "mongoose": "^5.11.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.5.1",
    "query-string": "^6.13.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "url-parse": "^1.4.7",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/cache-manager": "^3.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.11",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.171",
    "@types/node": "^14.14.36",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.10",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
